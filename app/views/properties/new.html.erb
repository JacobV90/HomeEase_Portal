<% content_for :side_content do %>
 <div class="container-fluid">
    <div class="side-body">
    <%= bootstrap_form_tag url: '/create_property' do |f| %>
      <%= f.text_field  :street %>
      <%= f.text_field :city %>
      <%= f.text_field :state %>
      <%= f.number_field :zipcode %>
      <%= f.number_field :price %>
      <%= f.number_field :bedrooms %>
      <%= f.number_field :bathrooms %>
      <%= f.text_field :amenities %>
      <%= f.text_field :description %>
      <%= f.submit "Add property" %>
   <% end %>
    </div>
  </div>
 
   
<%end%> 